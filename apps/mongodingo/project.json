{
  "name": "mongodingo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mongodingo",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/mongodingo"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/mongodingo"
        },
        "production": {}
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mongodingo:build",
        "dev": true,
        "port": 3000,
        "args": ["--no-turbo"]
      }
    }
  }
}
