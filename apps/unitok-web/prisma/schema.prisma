generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(cuid())
  clerkId   String   @unique //nemsen
  email     String   @unique
  name      String
  createdAt DateTime @default(now())
}
